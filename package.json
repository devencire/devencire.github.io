{
  "name": "devencire.github.io",
  "private": true,
  "description": "Maybe a blog.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn generate && gatsby build",
    "deploy": "yarn generate && gatsby build && gh-pages -d public -b master",
    "develop": "concurrently \"gatsby develop\" \"yarn generate --watch\"",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "generate": "wait-on http://localhost:8000/___graphql && graphql-codegen"
  },
  "dependencies": {
    "gatsby": "^2.8.3",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-plugin-typography": "^2.2.13",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-sutro": "^0.16.19"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.2.1",
    "@graphql-codegen/typescript-operations": "^1.2.1",
    "@types/graphql": "^14.2.1",
    "@types/typography": "^0.16.2",
    "concurrently": "^4.1.0",
    "gh-pages": "^2.0.1",
    "graphql": "^14.3.1",
    "prettier": "^1.17.1",
    "wait-on": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devencire/devencire.github.io"
  }
}
